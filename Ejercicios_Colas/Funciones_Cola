#include <iostream>
#include <queue>

using namespace std;

int main()
{
    queue<int> Myqueue;
    Myqueue.push(1);
    Myqueue.push(2);
    Myqueue.push(3);
    Myqueue.push(4);
    Myqueue.push(5);
    cout << "El tamano de la cola es de: " << Myqueue.size() << endl;
    cout << "El elemento de hasta arriba es : " << Myqueue.front() << endl;
    Myqueue.pop();
    cout << "El nuevo elemento de hasta arriba es : " << Myqueue.front() << endl;
    cout << "El nuevo tamano de la cola es de: " << Myqueue.size() << endl;
    return 0;
}
